rev = int(input("Введите выручку фирмы: "))
exp = int(input("Введите издержки фирмы: "))
if rev > exp:
    prof = rev - exp
    print(f"Финансовый результат - прибыль. Ее величина: {prof}")
    rat = prof / rev * 100
    print(f"Рентабельность выручки = {rat}")
    empl = int(input("Численность сотрудников фирмы: "))
    revempl = rat / empl
    print(f"Прибыль фирмы в расчете на одного сотрудника = {revempl}")
else:
    print(f"Финансовый результат - издержки. Их величина: {exp}")

